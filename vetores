#include <stdio.h>
#include <stdlib.h>

int main()
{

  int vet_numero[5];
  int cont;
  
  vet_numero[0] = 7;
  vet_numero[1] = 11;
  vet_numero[2] = 5;
  vet_numero[3] = 2;
  vet_numero[4] = 9;
  
  printf("\n Numero = %i",vet_numero[2]);
  
  cont = 3;
  
  printf("\n Numero = %i", vet_numero[cont]); 
  
  printf ("\n\n\n"); 
  
  system("PAUSE");	
  return 0;
}
